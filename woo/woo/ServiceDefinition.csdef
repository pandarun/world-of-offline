<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="woo" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2013-10.2.2">
  <WebRole name="web" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="RemoteAccess" />
      <Import moduleName="RemoteForwarder" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="ServiceBus" />
      <Setting name="Blob" />
    </ConfigurationSettings>
    <LocalResources>
      <LocalStorage name="WriteStorage" cleanOnRoleRecycle="true" sizeInMB="2048" />
      <LocalStorage name="ReadStorage" cleanOnRoleRecycle="true" sizeInMB="2048" />
    </LocalResources>
  </WebRole>
</ServiceDefinition>